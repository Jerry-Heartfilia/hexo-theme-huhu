@import 'reset.styl'
@import 'font.styl'
@import 'index.styl'

#main {
    width: 100%
    height: 100%
    background-color: black

    .title {
        color: #fff
        font-size: 48px
        font-family: 'California-Medium'
        text-shadow: 0 0 42px #2f9d9c
        -webkit-transform: skewY(-11.5deg)
        transform: skewY(-11.5deg)
        pointer-events: none // 去除鼠标事件
    }
}